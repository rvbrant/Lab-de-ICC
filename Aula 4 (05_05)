#include <stdio.h>
#include <stdlib.h>



void LeSensores(void);

void IA(void);
void DriveMotors(void);


int main()
{
int S1, S2, S3, M1, M2;

void LeSensores(void){
    int ss1, ss2, ss3, distancia1, distancia2, distancia3;
    scanf("%d %d %d", &distancia1, &distancia2, distancia3);

    if(distancia1<50)
        ss1 = 1;
    else
        ss1 = 0;

    if(distancia2<50)
        ss2 = 1;
    else
        ss2 = 0;

    if(distancia3<50)
        ss3 = 1;
    else
        ss3 = 0;
}

void IA(void){
    if (S1 == 0 && S2 == 0 && S3 == 0){
        M1 == 1;
        M2 == 1;
    }

    if (S1 == 0 && S2 == 1 && S3 == 0){
        M1 == 0;
        M2 == 1;
    }

    if (S1 == 1 && S2 == 0 && S3 == 0){
        M1 == 1;
        M2 == 0;
    }

    if (S1 == 0 && S2 == 0 && S3 == 1){
        M1 == 0;
        M2 == 1;
    }

    if (S1 == 1 && S2 == 1 && S3 == 1){
        M1 == 0;
        M2 == 1;
    }

    if (S1 == 1 && S2 == 1 && S3 == 0){
        M1 == 1;
        M2 == 0;
    }

    if (S1 == 0 && S2 == 1 && S3 == 1){
        M1 == 0;
        M2 == 1;
    }

    if (S1 == 1 && S2 == 0 && S3 == 1){
        M1 == 1;
        M2 == 1;
    }
}

void DriveMotors(void);
    printf("M1 = %d", M1);
    printf("M2 = %d", M2);

}
